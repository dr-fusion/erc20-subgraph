    "create-remote-v3": "graph create georgeroman/erc20-subgraph --node https://deploy.graphv3.summitx.finance",
    "deploy-remote-v4": "graph deploy georgeroman/erc20-subgraph --ipfs https://ipfs.graphv4.summitx.finance --node https://deploy.graphv4.summitx.finance",
    "create-remote-v4": "graph create georgeroman/erc20-subgraph --node https://deploy.graphv4.summitx.finance",
    "deploy-remote-goldsky":"goldsky subgraph deploy georgeroman-erc20-subgraph/1.0.0 --path build/",
